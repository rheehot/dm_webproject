]);\r\n\r\n    return (\r\n        <Home>\r\n            <Title>회원가입</Title>\r\n            <div style={{ margin: \"0 auto\" }}>\r\n                <Input onChange={onChangeId} placeholder=\"사용할 ID를 넣어주세요\" />\r\n                <Input onChange={onChangePw} placeholder=\"사용할 PW를 넣어주세요\" type=\"password\" />\r\n                <Input onChange={onChangePwCheck} placeholder=\"사용할 PW를 다시 입력해주세요\" type=\"password\" />\r\n                {\r\n                    state !== true ? <p style={{ color: 'red', fontWeight: 'bold' }}>비밀번호가 일치하지 않습니다!!</p>\r\n                        : <p style={{ color: 'white', fontWeight: 'bold' }}>비밀번호가 일치합니다!!</p>\r\n                }\r\n                <Submit onClick={onClickSubmit}>회원가입</Submit>\r\n            </div>\r\n        </Home>\r\n    )\r\n}\r\n\r\nexport default SignUp;","C:\\Users\\217-Pro\\Desktop\\jh\\dm_webproject\\front\\src\\components\\login.js",["96"],"import React, { useState, useEffect } from 'react';\r\nimport { Home, Title, InputId, InputPw, LoginBtn, LoginForm, SignUp } from './styled/styledLogin';\r\n\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { loginAction } from '../redux/members';\r\n\r\nimport { useHistory, Link } from 'react-router-dom';\r\n\r\nconst Login = () => {\r\n    const [login, setLogin] = useState({ id: null, pw: null });\r\n    const onChangeId = e => setLogin({ ...login, id: e.target.value });\r\n    const onChangePw = e => setLogin({ ...login, pw: e.target.value });\r\n\r\n    const dispatch = useDispatch();\r\n    const onClickLogin = () => {\r\n        dispatch(loginAction(login.id, login.pw));\r\n    }\r\n\r\n    const history = useHistory();\r\n    const loginStatus = useSelector(state => state.members.isLogin);\r\n    useEffect(() => {\r\n        loginStatus === true && history.push('/question');\r\n    }, [loginStatus]);\r\n\r\n    return (\r\n        <Home>\r\n            <Title>LOGIN</Title>\r\n            <LoginForm>\r\n                <InputId placeholder=\"ID\" onChange={onChangeId} />\r\n                <InputPw placeholder=\"PW\" type=\"password\" onChange={onChangePw} />\r\n                <LoginBtn onClick={onClickLogin}>로그인</LoginBtn>\r\n                <SignUp><Link to=\"/signUp\">회원가입</Link></SignUp>\r\n            </LoginForm>\r\n        </Home>\r\n    )\r\n}\r\n\r\nexport default Login;","C:\\Users\\217-Pro\\Desktop\\jh\\dm_webproject\\front\\src\\components\\result.js",["97","98"],"C:\\Users\\217-Pro\\Desktop\\jh\\dm_webproject\\front\\src\\components\\question.js",["99","100","101"],"import React, { useState, useEffect } from 'react';\r\nimport { Home, Title, Questions, ButtonGroup, Button } from './styled/styleQuestion';\r\nimport { useHistory } from 'react-router-dom';\r\nimport { useDispatch, useSelector } from 'react-redux';\r\nimport { questionAction } from '../redux/question';\r\n\r\nimport Layout from '../Layout';\r\n\r\nconst Question = () => {\r\n    const [state, setState] = useState([]);\r\n    const [count, setCount] = useState(0);\r\n    const history = useHistory();\r\n\r\n    const onClickBtn = (e) => {\r\n        setCount(count + 1);\r\n        setState([...state, e.target.dataset.type === 'yes' ? true : false]);\r\n    }\r\n\r\n    const dispatch = useDispatch();\r\n    useEffect(() => {\r\n        setTimeout(() => {\r\n            count !== 0 && dispatch(questionAction(state));\r\n        }, 1500);\r\n    }, [count < 5]);\r\n\r\n    const resultLoading = useSelector(state => state.question.loading);\r\n    useEffect(() => {\r\n        resultLoading === true && history.push('/result');\r\n    }, [resultLoading]);\r\n\r\n    return (\r\n        count < 5 ?\r\n            <Home>\r\n                <Layout />\r\n                <Title>{dummy.data[count].title}</Title>\r\n                <Questions>{dummy.data[count].desc}</Questions>\r\n                <ButtonGroup>\r\n                    <Button onClick={onClickBtn} data-type=\"yes\">Yes</Button>\r\n                    <Button onClick={onClickBtn} d